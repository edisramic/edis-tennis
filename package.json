{
  "name": "edis-tennis",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "compile": "tsc src/index.ts",
    "simulate": "node src/index.js"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  }
}
